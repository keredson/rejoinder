{
  "name": "Rejoinder",
  "description": "Google Meet emoji tool, similar to Google's internal MeetMoji extension.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
 "content_scripts": [
   {
     "matches": ["https://meet.google.com/*"],
     "css": ["meet.css"],
     "js": ["meet.js"]
   }
 ],
  "web_accessible_resources": [
    {
      "resources": ["bip.mp3"],
      "matches": ["https://meet.google.com/*"]

    }
  ]
}